{
  "name": "netlab",
  "version": "0.6.0",
  "dependencies": {
    "es6-shim": "^0.22.2",
    "express": "~4.4.5",
    "formidable": "^1.0.16",
    "johnny-five": "~0.8.78",
    "node-osc": "^0.3.0",
    "phantomjs": "^1.9.13",
    "request": "~2.21.0",
    "socket.io": "~1.0.0",
    "underscore": "~1.4.4"
  },
  "devDependencies": {
    "matchdep": "~0.1.2",
